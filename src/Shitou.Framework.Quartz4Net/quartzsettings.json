{
  "QuartzConfig": {
    //任务
    "Jobs": [
      {
        //任务名
        "Name": "FirstJob",
        //分组
        "Group": "Default",
        //任务类型(全限定名)
        "Type": "Quartz4NetTest.Jobs.FirstJob",
        //指定Triggers中配置触发器名称
        "Trigger": "FirstTrigger",
        //是否启用
        "Enabled": true
      },
      {
        "Name": "SecondJob",
        "Group": "Default",
        "Type": "Quartz4NetTest.Jobs.SecondJob",
        "Trigger": "SecondTrigger",
        "Enabled": true
      }

    ],
    //触发器
    "Triggers": [
      {
        "Name": "FirstTrigger",
        "Group": "Default",
        "CronExpression": "0/5 * * * * ? *",
        "StartTime": null,
        "EndTime": null
      },
      {
        "Name": "SecondTrigger",
        "Group": "Default",
        "CronExpression": "0/10 * * * * ? *",
        "StartTime": null,
        "EndTime": null
      }
    ]
  }
}
